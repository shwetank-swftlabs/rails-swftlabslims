<% content_for :page_title, render("shared/page_title", 
  subtitle: "View feedstock details.",
  actions: render("shared/print_qr_button", qr_code_path: qr_code_inventory_feedstock_path(@feedstock))
) %>

<%= render "shared/sidebar_layout/layout",
    tabs: ["details", "data_files", "images", "use_records", "comments"],
    tab_path: :inventory_feedstock_path,
    path_args: [@feedstock],
    bottom_action: render("shared/edit_button", label: "Edit Feedstock", path: edit_inventory_feedstock_path(@feedstock)) do %>

  <%= render (params[:tab] || "details"), feedstock: @feedstock %>

<% end %>

