<%= render "shared/sidebar_layout/details_list_main_area", details: {
  "Name" => feedstock.name,
  "Type" => feedstock.feedstock_type.humanize,
  "Quantity Added" => "#{feedstock.quantity} #{feedstock.unit.upcase}",
  "Location" => feedstock.location.present? ? feedstock.location : "N/A",
  "Supplier" => feedstock.supplier.present? ? feedstock.supplier : "N/A",
  "Created By" => feedstock.created_by,
  "Created At" => feedstock.created_at.strftime("%B %d, %Y %I:%M %p")
}
%>

